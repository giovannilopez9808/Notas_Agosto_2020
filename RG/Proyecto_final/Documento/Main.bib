@article{Weisberg2010,
  abstract      = {We present results of more than three decades of timing measurements of the first known binary pulsar, PSR B1913+16. Like most other pulsars, its rotational behavior over such long timescales is significantly affected by small-scale irregularities not explicitly accounted for in a deterministic model. Nevertheless, the physically important astrometric, spin, and orbital parameters are well determined and well decoupled from the timing noise. We have determined a significant result for proper motion, $\mu$$\alpha$ = - 1.43 ±0.13,$\mu$$\delta$ =-0.70 ±0.13 mas yr-1. The pulsar exhibited a small timing glitch in 2003 May, with $\Delta$f/f = 3.7 × 10-11, and a smaller timing peculiarity in mid-1992. A relativistic solution for orbital parameters yields improved mass estimates for the pulsar and its companion, m1 = 1.4398 ± 0.0002 M⊙ and m2 = 1.3886 ± 0.0002 M⊙. The system's orbital period has been decreasing at a rate 0.997 ± 0.002 times that predicted as a result of gravitational radiation damping in general relativity. As we have shown before, this result provides conclusive evidence for the existence of gravitational radiation as predicted by Einstein's theory. {\textcopyright} 2010. The American Astronomical Society.},
  archiveprefix = {arXiv},
  arxivid       = {1011.0718},
  author        = {Weisberg, J. M. and Nice, D. J. and Taylor, J. H.},
  doi           = {10.1088/0004-637X/722/2/1030},
  eprint        = {1011.0718},
  file          = {:home/giovanni/Music/Weisberg{\_}2010{\_}ApJ{\_}722{\_}1030.pdf:pdf},
  issn          = {15384357},
  journal       = {Astrophysical Journal},
  keywords      = {Binaries: close,Gravitation,Stars: kinematics and dynamics},
  number        = {2},
  pages         = {1030--1034},
  title         = {{Timing measurements of the relativistic binary pulsar PSR B1913+16}},
  volume        = {722},
  year          = {2010}
}
